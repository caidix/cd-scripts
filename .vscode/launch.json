{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "当前文件",
      "type": "node",
      "request": "launch",
      "program": "/usr/local/bin/ts-node",
      "args": ["${relativeFile}"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "build",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "build"
      ],
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
